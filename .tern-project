{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "ecma5",
    "ecma6",
    "ecma7"
  ],
  "loadEagerly": [
    "node_modules/react-dom/**/*.js",
    "node_modules/react/**/*.js",
    "app/javascript/**/*.js",
    "app/javascript/**/*.jsx"
  ],
  "plugins": {
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
